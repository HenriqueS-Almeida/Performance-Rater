package model;

import java.sql.Date;
import java.sql.Time;

public class Agenda {
	private String nome;
	private Date data;
	private Time hora;
	private String materia;
	private int aluno;
	
	public Agenda() {
	}
	
	public Agenda(String nome, Date data, Time hora, String materia, int aluno) {
		super();
		this.nome = nome;
		this.data = data;
		this.hora = hora;
		this.materia = materia;
		this.aluno = aluno;
	}

	public String getNome() {
		return nome;
	}
	
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	public Date getData() {
		return data;
	}
	
	public void setData(Date data) {
		this.data = data;
	}
	
	public Time getHora() {
		return hora;
	}
	
	public void setHora(Time hora) {
		this.hora = hora;
	}
	
	public String getMateria() {
		return materia;
	}
	
	public void setMateria(String materia) {
		this.materia = materia;
	}
	
	public int getAluno() {
		return aluno;
	}
	
	public void setAluno(int aluno) {
		this.aluno = aluno;
	}
}
